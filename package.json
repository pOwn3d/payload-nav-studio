    },
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@payloadcms/translations": "^3.77.0",
    "@payloadcms/ui": "^3.77.0",
    "@types/react": "^19.0.0",
    "payload": "^3.77.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  }
}
